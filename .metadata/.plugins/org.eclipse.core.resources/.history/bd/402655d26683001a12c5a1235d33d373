package objectcastingtopic;
class Sample1
{
	int a = 10;
	public void test1()
	{
		System.out.println("Running test1 method of parent");
	}
}
class Sample2 extends Sample1
{
	int a = 10;
	public void test1()
	{
		System.out.println("Running test1 method of parent");
	}
}
public class Tester2
{
	public static void main(String[] args)
	{
		
	}
}
